all:
	g++ main.cpp -fopenmp -Wall -O3 -std=c++11 -o main
	#g++ main_negsamp1.cpp -fopenmp -Wall -O3 -std=c++11 -o main_negsamp1
	#g++ main_negsamp2.cpp -fopenmp -Wall -O3 -std=c++11 -o main_negsamp2
